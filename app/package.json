{
  "name": "about-frontend",
  "version": "1.0.0",
  "description": "Aragon About app, from Autark",
  "main": "src/index.js",
  "dependencies": {
    "@aragon/api": "2.0.0-beta.8",
    "@aragon/api-react": "2.0.0-beta.8",
    "@aragon/ui": "1.0.0",
    "@babel/polyfill": "^7.7.0",
    "@githubprimer/octicons-react": "^8.5.0",
    "axios": "^0.19.0",
    "codemirror": "^6.0.0",
    "dompurify": "^1.0.11",
    "ipfs-http-client": "^30.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.2.2",
    "rxjs": "^6.5.3",
    "styled-components": "^4.4.1",
    "web3-utils": "^1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "parcel-bundler": "^1.12.4"
  },
  "scripts": {
    "build": "npm run sync-assets && npm run build:script && npm run build:app",
    "build:app": "parcel build index.html -d ../dist/ --public-url \".\"",
    "build:script": "parcel build src/script.js -d ../dist/",
    "copy-public-assets": "rsync -rtu ./public/ ../dist",
    "watch:script": "parcel watch src/script.js --out-dir ../dist/ --no-hmr",
    "devserver": "npm run build -- --no-minify && parcel serve index.html -p 8001 -d ../dist/ --no-cache",
    "start": "npm run sync-assets && npm run build:script -- --no-minify && npm run devserver",
    "sync-assets": "copy-aragon-ui-assets ../dist && npm run copy-public-assets"
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AutarkLabs/about.git"
  },
  "keywords": [],
  "license": "GPL-3.0"
}
