{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": [
          "> 1%",
          "last 3 versions",
          "ie >= 9",
          "ios >= 8",
          "android >= 4.2"
        ]
      },
      "useBuiltIns": false
      }]
  ],
  "plugins": [
    ["babel-plugin-styled-components", {
      "displayName": true,
      "pure": true
      }],
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel"
  ]
}
